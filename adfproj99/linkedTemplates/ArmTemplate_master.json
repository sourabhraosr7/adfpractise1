{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adfproj99"
		},
		"LS_PARQ_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_PARQ'"
		},
		"LS_PROD_SCD_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_PROD_SCD'"
		},
		"LS_TXT_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'LS_TXT'"
		},
		"SqlServer1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer1'"
		},
		"SqlServer2_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'SqlServer2'"
		},
		"LS_PARQ_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://str1234s.dfs.core.windows.net/"
		},
		"LS_PROD_SCD_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://str1234s.dfs.core.windows.net/"
		},
		"LS_TXT_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://str1234s.dfs.core.windows.net/"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adfproj99_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2018-02-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"LS_PARQ_accountKey": {
						"value": "[parameters('LS_PARQ_accountKey')]"
					},
					"LS_PROD_SCD_accountKey": {
						"value": "[parameters('LS_PROD_SCD_accountKey')]"
					},
					"LS_TXT_accountKey": {
						"value": "[parameters('LS_TXT_accountKey')]"
					},
					"SqlServer2_connectionString": {
						"value": "[parameters('SqlServer2_connectionString')]"
					},
					"SqlServer1_connectionString": {
						"value": "[parameters('SqlServer1_connectionString')]"
					},
					"LS_PARQ_properties_typeProperties_url": {
						"value": "[parameters('LS_PARQ_properties_typeProperties_url')]"
					},
					"LS_PROD_SCD_properties_typeProperties_url": {
						"value": "[parameters('LS_PROD_SCD_properties_typeProperties_url')]"
					},
					"LS_TXT_properties_typeProperties_url": {
						"value": "[parameters('LS_TXT_properties_typeProperties_url')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}